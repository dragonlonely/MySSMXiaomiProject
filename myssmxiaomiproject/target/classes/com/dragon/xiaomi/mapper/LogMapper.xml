<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dragon.xiaomi.mapper.LogMapper">

    <insert id="addLog" parameterType="com.dragon.xiaomi.log.pojo.TbLog">
        insert into tb_log(loginname,operationname,operationtype,createtime,result) values (#{loginname},#{operationname},#{operationtype},#{createtime},#{result})
    </insert>
</mapper>